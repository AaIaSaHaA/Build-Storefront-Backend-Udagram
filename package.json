{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "## Getting Started",
  "main": "server.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node build/server.js",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess \"node ./dist/server.js\"",
    "test": "set ENV=test&& db-migrate --env test up && jasmine-ts && db-migrate db:drop test",
    "tsc": "tsc",
    "mytest": "npm run build && npm run jasmine",
    "jasmine": "jasmine",
    "prettier": "prettier --config .prettierrc ./src/**/*.ts --write",
    "test-up": "db-migrate --config ./database.json --e test up",
    "test-teardown": "db-migrate reset --config ./database.json --e test",
    "test-db": "npm run testdb-down && export NODE_ENV=test || set NODE_ENV=test && npm run testdb-up && jasmine-ts src/models/tests/db_spec.ts && npm run test-teardown",
    "test-routes": "npm run testdb-down && export NODE_ENV=test || set NODE_ENV=test && npm run testdb-up && jasmine-ts src/server-tests.ts && npm run test-teardown",
    "final-test": ""
  },
  "author": "Udacity",
  "license": "ISC",
  "dependencies": {
    "@marblejs/middleware-logger": "^4.0.3",
    "@types/jwt-decode": "^3.1.0",
    "@types/logger": "^0.0.1",
    "@types/redux-logger": "^3.0.9",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "1.2.2",
    "dotenv": "16.0.3",
    "express": "^4.17.1",
    "jsonwebtoken": "9.0.0",
    "jwt-decode": "^3.1.2",
    "lodash": "4.17.21",
    "logger": "^0.0.1",
    "pg": "8.10.0",
    "prettier": "2.8.4",
    "redux-logger": "^3.0.6",
    "supertest": "6.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "8.2.0",
    "@types/express": "^4.17.9",
    "@types/jasmine": "3.10.7",
    "@types/jsonwebtoken": "9.0.1",
    "@types/lodash": "4.14.191",
    "@types/node": "^18.11.18",
    "@types/pg": "^7.14.7",
    "@types/supertest": "2.0.12",
    "jasmine": "3.99.0",
    "jasmine-spec-reporter": "6.0.0",
    "jasmine-ts": "^0.3.0",
    "ts-node": "10.9.1",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.9.4"
  },
  "keywords": []
}
